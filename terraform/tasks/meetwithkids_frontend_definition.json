[
  {
    "name": "frontend-container",
    "image": "700347967260.dkr.ecr.ap-northeast-1.amazonaws.com/meetwithkids-frontend",
    "essential": true,
    "firelensConfiguration": {
      "type": "fluentbit",
      "options":{
         "enable-ecs-log-metadata": "true"
      }
    },
    "logConfiguration": {
      "logDriver": "awsfirelens",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "frontend",
        "awslogs-group": "/ecs/frontend"
      }
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 80
      }
    ],
    "command": [
      "yarn", "start"
    ],
    "environment": [
      {
        "name": "WORKDIR",
        "value": "app"
      },
      {
        "name": "CONTAINER_PORT",
        "value": "80"
      },
      {
        "name": "PORT",
        "value": "80"
      }
    ]
  }
]
